html(ng-app="home")
    head(lang='en')
        meta(charset='UTF-8')
        title
        link(rel='stylesheet', href='/css/home.css')
        link(rel='stylesheet', href='/css/bootstrap.css')
        link(rel='stylesheet', href='/css/bootstrap-theme.css')
        script(src='/js/jquery-2.1.3.min.js')
        script(src='/js/bootstrap.min.js')
        script(src='/js/angular.min.js')
        script(src='/js/angular-route.js')
        script(src="/socket.io/socket.io.js")
        script(src='/js/home.js')
    body.container-fluid(ng-controller="page")
        .row
            .col-md-12.head
                .headtext
                    |Welcome to Wonderland!
        .row
            .col-md-3.pull-right
                img.frame.col-md-12(src="/img/frame_avatar.png")
                img.avatar.col-md-9(ng-src="{{user.avatar}}")
            .menuAll.col-md-3.pull-left
                img.menu.col-md-12(src="/img/top_menu.png")
                img.menu.col-md-12(src="/img/button_menu.jpg")
                img.menu.col-md-12(src="/img/button_menu.jpg")
                img.menu.col-md-12(src="/img/button_menu.jpg")
                img.menu.col-md-12(src="/img/button_menu.jpg")
                img.menu.col-md-12(src="/img/bottom_menu.jpg")

